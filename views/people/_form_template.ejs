<div class="form-group">
    <label>Person ID (e.g. Student ID): </label>
    <input type="text" name="studentID" value="<%= person.studentID %>" class="form-control">
</div>
<div class="form-group">
    <label>Full Name:</label>
    <input type="text" name="fullName" value="<%= person.fullName %>" class="form-control">
</div>
<div class="form-group">
    <label>Email Address:</label>
    <input type="text" name="emailAddress" value="<%= person.emailAddress %>" class="form-control">
</div>
<div class="form-group">
    <label>Phone Number: </label>
    <input type="text" name="phoneNumber" value="<%= person.phoneNumber %>" class="form-control">
</div>
<input type="submit" value="Save Person Info." class="btn btn-primary">