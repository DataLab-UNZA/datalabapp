<div class="form-group">
<label>Project Code: </label>
<input type="text" name="projectCode" value="<%= project.projectCode %>" class="form-control">
</div>
<div class="form-group">
<label>Project name:</label>
<input type="text" name="projectName" value="<%= project.projectName %>" class="form-control">
</div>
<div class="form-group">
<label>Project Description:</label>
<textarea name="projectDescription" cols="30" rows="10" class="form-control"><%= project.projectDescription %></textarea>
</div>
<div class="form-group">
    <label>Project Team Members:</label> <br>
</div>
<div class="form-group">
    <% interns.forEach((intern, index) => { %>
        <div class="form-check">
            <input type="checkbox" name="responsibility[<%= index %>]" value="<%= intern.fullName %>" class="form-check-input"><%= intern.fullName %> <br>
        </div>
    <% }); %>
</div>
<input type="submit" value="Save DataLab Project" class="btn btn-primary">